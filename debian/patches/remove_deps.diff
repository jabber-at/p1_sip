Description: remove deps in rebar.config
 Without this patch, rebar would download and compile the listed deps
 at compile time.
Author: Philipp Huebner <debalance@debian.org>

--- a/rebar.config
+++ b/rebar.config
@@ -1,11 +1,3 @@
-{erl_opts, [{i, "include"},
-	    {i, "deps/p1_stun/include"},
-	    {i, "../p1_stun/include"}]}.
-
-{deps, [{p1_stun, ".*", {git, "git://github.com/processone/stun.git"}},
-	{p1_tls, ".*", {git, "git://github.com/processone/tls.git"}},
-	{p1_utils, ".*", {git, "git://github.com/processone/p1_utils.git"}}]}.
-
 %% Local Variables:
 %% mode: erlang
 %% End:
